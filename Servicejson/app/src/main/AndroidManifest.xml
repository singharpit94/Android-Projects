<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arpit.servicejson" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Notify" />

        <activity
            android:name=".Testing"
            android:label="@string/title_activity_testing"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Zening"
            android:label="@string/title_activity_zening"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Final"
            android:label="@string/title_activity_final"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Hoja"
            android:label="@string/title_activity_hoja"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
